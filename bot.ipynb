    "import telebot\n",
    "from telebot import types\n",
    "import json\n",
    "import random\n",
    "\n",
    "TOKEN = '...',
    "bot = telebot.TeleBot(TOKEN)\n",
    "\n",
    "with open ('terms.json', encoding='utf-8') as th:\n",
    "    term = json.load(th)\n",
    "    terms = term['term']\n",
    "\n",
    "with open('quiz_2.json', encoding='utf-8') as fh:\n",
    "    data = json.load(fh)\n",
    "\n",
    "count = len(data['quiz'])\n",
    "var = 1\n",
    "score = 0 \n",
    "    \n",
    "@bot.message_handler(commands=['start'])\n",
    "def start(m):\n",
    "    keyboard=types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "    keyboard.add(*[types.KeyboardButton(name) for name in \n",
    "    ['–ü—Ä–æ–π—Ç–∏ —Ç–µ—Å—Ç üìù', '–£–∑–Ω–∞—Ç—å –±–æ–ª—å—à–µ! üëç']])\n",
    "    msg = bot.send_message(m.chat.id,'–ß—Ç–æ –≤—ã —Ö–æ—Ç–∏—Ç–µ? ü§î',\n",
    "    reply_markup=keyboard)    \n",
    "    print(msg['from_user'])\n",
    "    bot.register_next_step_handler(msg, blocks)\n",
    "\n",
    "\n",
    "def blocks(m):\n",
    "    global term\n",
    "    terms = term['term'] \n",
    "    keyboard=types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "    rnd = random.choice(terms)        \n",
    "    if m.text == '–£–∑–Ω–∞—Ç—å –±–æ–ª—å—à–µ! üëç':\n",
    "        msg = bot.send_message(m.chat.id, rnd,\n",
    "        reply_markup=keyboard) \n",
    "        bot.register_next_step_handler(msg, start)\n",
    "    elif m.text == '–ü—Ä–æ–π—Ç–∏ —Ç–µ—Å—Ç üìù':\n",
    "        keyboard=types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "        msg = bot.send_message(m.chat.id, '–ù–∞—á–Ω–µ–º! üìå –ù–∞–∂–º–∏—Ç–µ –Ω–∞ –∫–Ω–æ–ø–∫—É \"–ü—Ä–æ–π—Ç–∏ —Ç–µ—Å—Ç\" –µ—â—ë —Ä–∞–∑ üëá' , reply_markup=keyboard)\n",
    "        bot.register_next_step_handler(msg, quiz)\n",
    "    else:\n",
    "        keyboard=types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "        msg = bot.send_message(m.chat.id, '–Ø –Ω–µ –∑–Ω–∞—é —Ç–∞–∫–æ–π –∫–æ–º–∞–Ω–¥—ã üôÄ' , reply_markup=keyboard)\n",
    "        bot.register_next_step_handler(msg, start)\n",
    "        \n",
    "@bot.message_handler(commands=['what'])\n",
    "def terms(m):\n",
    "    global term\n",
    "    terms = term['term']\n",
    "    keyboard=types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "    rnd = random.choice(terms)\n",
    "    msg = bot.send_message(m.chat.id, rnd,\n",
    "        reply_markup=keyboard) \n",
    "    bot.register_next_step_handler(msg, start)\n",
    "\n",
    "@bot.message_handler(commands=['quiz'])\n",
    "def quiz(m):\n",
    "    keyboard=types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "    keyboard.add(*[types.KeyboardButton(name) for name in \n",
    "    data['quiz']['q1']['options']])\n",
    "    msg = bot.send_message(m.chat.id, data['quiz']['q1']['question'],\n",
    "    reply_markup=keyboard)\n",
    "    bot.register_next_step_handler(msg, name)\n",
    "\n",
    "def name(m):\n",
    "    global var\n",
    "    global score\n",
    "    keyboard = types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "    if m.text == data['quiz']['q'+str(var)]['answer']:\n",
    "        score = score + 1\n",
    "    var = var + 1\n",
    "    if var <= count:\n",
    "        keyboard=types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "        keyboard.add(*[types.KeyboardButton(name) for name in \n",
    "        data['quiz']['q'+str(var)]['options']])\n",
    "        msg = bot.send_message(m.chat.id, data['quiz']['q'+str(var)]['question'],\n",
    "        reply_markup=keyboard)   \n",
    "        bot.register_next_step_handler(msg, name)\n",
    "    else:\n",
    "        var = 1\n",
    "        msg = bot.send_message(m.chat.id, '#result üìç'+\n",
    "        '–í—ã –æ—Ç–≤–µ—Ç–∏–ª–∏ –ø—Ä–∞–≤–∏–ª—å–Ω–æ –Ω–∞: ' + str(score) + ' –≤–æ–ø—Ä–æ—Å–æ–≤ –∏–∑ ' + str(count),\n",
    "        reply_markup=keyboard)   \n",
    "\n",
    "@bot.message_handler(content_types=['text'])\n",
    "def other(m):\n",
    "    keyboard=types.ReplyKeyboardMarkup(resize_keyboard=True)\n",
    "    msg = bot.send_message(m.chat.id,'–Ø –Ω–µ –∑–Ω–∞—é —Ç–∞–∫–æ–π –∫–æ–º–∞–Ω–¥—ã üôÄ',\n",
    "    reply_markup=keyboard)\n",
    "    bot.register_next_step_handler(msg, start)\n",
    "    \n",
    "bot.polling()"
   ]
